<Header 
  @title="Roster" 
  @iconclick={{action this.clickReturn}} 
/>

<main>
  <SearchBar @searchtext={{this.searchtext}} />

  <RosterList class="page-frame">
    {{#each candidates as |participant| }}
      <RosterRow 
        @model={{participant}} 
        @selection={{action "clickReturn"}} 
      />
    {{/each}}
  </RosterList>
</main>

<NavigationBar>
  <NavigationItem 
    @text="Back" 
    @icon="users" 
    @itemclick={{action "clickReturn"}}
  />
</NavigationBar>
