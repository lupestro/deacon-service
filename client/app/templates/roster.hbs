<Header 
  @title="Roster" 
  @iconclick={{this.clickReturn}} 
/>

<main>
  <SearchBar @searchtext={{this.searchtext}} @update={{this.update}} />

  <RosterList class="page-frame">
    {{#each this.candidates as |participant| }}
      <RosterRow 
        @model={{participant}} 
        @selection={{this.clickReturn}} 
      />
    {{/each}}
  </RosterList>
</main>

<NavigationBar>
  <NavigationItem 
    data-test="Back"
    @text="Back" 
    @icon="users" 
    @itemclick={{this.clickReturn}}
    @itempress={{this.clickReturn}}
  />
</NavigationBar>
