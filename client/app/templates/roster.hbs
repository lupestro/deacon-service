<div class="page">
  <h2 class="supertitle">First Church Deacons</h2>
  {{duties-header title="Roster" iconclick=(action clickReturn) }}
  {{search-bar searchtext=searchtext}}

  {{#roster-list class="page-frame"}}
    {{#each candidates as |participant| }}
      {{roster-row model=participant selection=(action clickReturn) }}
    {{/each}}
  {{/roster-list}}

  {{#selection-bar}}
    {{selection-item 
      text="Back" 
      icon="users" 
      itemclick=(action clickReturn)
    }}
  {{/selection-bar}}

</div>
