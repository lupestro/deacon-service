  <Header 
    @title={{this.title}}
    @icon="calendar-alt" 
    @iconclick={{action this.clickCalendar}}
  />

  {{outlet}}

  <NavigationBar>
    <NavigationItem 
      @text="Me" 
      @icon="user" 
      @itemclick={{action this.clickMyDuties}} 
      @itempress={{action this.clickNewMe}} 
    />
    <NavigationItem 
      @text="Family" 
      @icon="bed" 
      @itemclick={{action this.clickFamilyDuties}}
    />
    <NavigationItem 
      @text="All" 
      @icon="users" 
      @itemclick={{action this.clickAllDuties}}
    />
    <NavigationItem 
      @text="Subs" 
      @icon="exchange-alt" 
      @itemclick={{action this.clickDutySubstitutions}}
    />
    <NavigationItem 
      @class="any-user"
      @text="Any" 
      @icon="user-plus" 
      @itemclick={{action this.clickAnyPersonDuties}}
    />
  </NavigationBar>
