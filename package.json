{
  "name": "deacon-service",
  "version": "0.0.0",
  "private": true,
  "description": "Deacon client and server",
  "license": "MIT",
  "author": "Ralph Mack",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lupestro/deacon-service.git"
  },
  "scripts": {
    "install": "(cd client && npm install) && (cd server && npm install)",
    "start": "concurrently \"cd client && PORT=4200 npm run start\" \"cd server && PORT=5000 npm run start\"",
    "build": "concurrently \"cd client && npm run build\" \"cd server && npm run build\"",
    "start:prod": "cd server && npm run start:prod"
  },
  "devDependencies": {},
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "keywords": [
    "church",
    "deacons"
  ],
  "bugs": {
    "url": "https://github.com/lupestro/deacon-service/issues"
  },
  "homepage": "https://github.com/lupestro/deacon-service#readme",
  "dependencies": {
    "concurrently": "^3.6.0"
  }
}
