{
  "name": "deacon-server",
  "version": "0.1.0",
  "private": true,
  "description": "Deacon server app",
  "license": "MIT",
  "author": "Ralph Mack",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "main": "app.js",
  "scripts": {
    "build": "npx tsc",
    "build:prod": "npx tsc",
    "lint": "npx eslint .",
    "docs": "npx jsdoc -c ./jsdoc.json",
    "start": "npx tsc && node ./src/app.js",
    "debug": "npx tsc && node --inspect-brk=36942 ./src/app.js",
    "test": "npx tsc && mocha test/**/*.js",
    "cover": "npx nyc -r HTML mocha test/**/*.js",
    "debug:test": "npx tsc && npx mocha --inspect-brk=36942 test/**/*.js"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.6.0",
    "eslint-plugin-node": "^11.1.0",
    "jsdoc": "^3.6.3",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "npm": "^6.1.0",
    "nyc": "^15.0.1",
    "typedoc": "^0.17.3",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "pg": "^8.0.0",
    "xlsx": "^0.15.2"
  },
  "resolutions": {
    "handlebars": "4.7.6",
    "minimist": "1.2.5"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  }
}
